{
  "version": 2,
  "routes": [
    {
      "src": "/future-proof-data-fetching-with-resource-hooks/?",
      "status": 308,
      "headers": { "location": "/future-proof-data-fetching-with-react" }
    },
    {
      "src": "/giving-names-meaning/?",
      "status": 308,
      "headers": { "location": "/how-to-give-meaningful-names" }
    },
    {
      "src": "/static/(.*)",
      "headers": { "cache-control": "public, max-age=31536000, immutable" }
    },
    {
      "src": "/page-data/(.*)",
      "headers": { "cache-control": "public, max-age=0, must-revalidate" }
    },
    {
      "src": "/app-data.json",
      "headers": { "cache-control": "public, max-age=0, must-revalidate" }
    },
    {
      "src": "/(.*\\.(js|json|css|ico|png)$)",
      "headers": { "cache-control": "public, max-age=31536000, immutable" }
    },
    {
      "src": "/(.*)",
      "headers": { "cache-control": "public, max-age=0, must-revalidate" }
    }
  ],
  "builds": [
    { "use": "@now/static-build", "src": "package.json", "config": { "distDir": "public" } }
  ]
}
